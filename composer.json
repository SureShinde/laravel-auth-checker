{
    "name": "sureshinde/laravel-auth-checker",
    "description": "Laravel Auth Checker allows you to log users authentication, devices authenticated from and lock intrusions.",
    "type": "library",
    "keywords": [
        "laravel",
        "package",
        "plugin",
        "authentication",
        "user",
        "check",
        "device",
        "ip",
        "laravel-package",
        "laravel-plugin"
    ],
    "require": {
        "php": "^8.0",
        "laravel/framework": "^10.0",
        "sureshinde/mobile-desktop-detect": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6",
        "orchestra/testbench": "^8.0",
        "sureshinde/orchestra-database": "^7.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Suresh Shinde",
            "email": "suresh@sureshinde.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "SureShinde\\AuthChecker\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SureShinde\\AuthChecker\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SureShinde\\AuthChecker\\AuthCheckerServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
